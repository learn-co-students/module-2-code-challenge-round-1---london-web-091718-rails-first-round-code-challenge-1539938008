<h2 id="title">All Heroines</h2>

<div id="search-bar">
<%= form_tag("/heroines", method: "get") do %>
  <%= label_tag(:search, "Search by Power") %>
  <%= text_field_tag(:search) %>
  <%= submit_tag("Search") %>
<% end %>
</div>



<ul>
  <% @heroines.each do |heroine| %>
    <li>
      <%= link_to heroine.super_name, heroine_path(heroine)  %>
    </li>
    <br>
  <% end %>
</ul>

<h4><%= link_to "Create a Heroine", new_heroine_path %></h4>

<h5><%= link_to "Powers", powers_path  %></h5>
